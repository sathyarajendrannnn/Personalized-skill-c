#include <stdio.h>
#include <ctype.h>

int main() {
    
    int i = 0, j = 0;
    char input[100], output[100];
    printf("Enter a string: ");
    scanf("%[^\n]", input);

    while (input[i] != '\0') {
        if (!isalpha(input[i])) {  
            output[j++] = input[i];
        }
        i++;  
    }
    output[j] = '\0';  
    printf("%s\n", output);

    return 0;
}
